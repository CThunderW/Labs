{
  "name": "knex_labs",
  "version": "1.0.0",
  "description": "integrate postgreSQL with Knex",
  "main": "knexfile.js",
  "scripts": {
    "start": "nodemon app.js",
    "db:create": "createdb -e express_demo",
    "db:drop": "dropdb -e --if-exists express_demo",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:repl": "psql -d express_demo"
  },
  "author": "Cale Williams",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "knex": "^0.15.2",
    "method-override": "^3.0.0",
    "pg": "^7.4.3",
    "popper": "^1.0.1",
    "psql": "0.0.1"
  },
  "devDependencies": {}
}
